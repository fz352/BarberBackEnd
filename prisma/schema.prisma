generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./barber.db"
}

model Services {
  id       Int    @id @default(autoincrement())
  name     String
  duration Int
  price    Float
}

model Notifications {
  id               Int      @id @default(autoincrement())
  dateTimeCreated  DateTime @default(now())
  telephone        String
  status           String
  text             String
  DateTimeSend     DateTime?
}