generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Notifications {
 id          Int         @id @default(autoincrement())
 dateTimeCreated DateTime  @default(now())
 telephone   String
 status String
 text String
 DateTimeSend DateTime?
}
  



